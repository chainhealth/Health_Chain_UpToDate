{
    "name": "health_chain",
    "version": "1.0.0",
    "description": "chaincode for health_chain",
    "main": "index.js",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "eslint *.js */**.js",
        "pretest": "npm run lint",
        "test": "nyc mocha --recursive",
        "start": "fabric-chaincode-node start"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-contract-api": "^2.5.4",
        "fabric-shim": "^2.5.4",
        "json-stringify-deterministic": "^1.0.12",
        "sort-keys-recursive": "^2.1.10"
    },
    "devDependencies": {
        "chai": "^4.3.10",
        "eslint": "^8.56.0",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "sinon": "^17.0.1",
        "sinon-chai": "^3.7.0"
    },
    "nyc": {
        "exclude": [
            "coverage/**",
            "test/**",
            "index.js",
            ".eslintrc.js"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}